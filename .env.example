# LLM Configuration (Required for photo validation)
OPENAI_API_KEY=your_openai_api_key_here
# Or alternatively, use xAI
# XAI_API_KEY=your_xai_api_key_here
# LLM_BASE_URL=https://api.x.ai/v1
LLM_BASE_URL=https://api.openai.com/v1
LLM_MODEL=gpt-4o

# Database Configuration (for future use)
DATABASE_URL=postgresql://username:password@localhost:5432/base_power_survey

# AWS Configuration (for future use)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_S3_BUCKET=base-power-survey-photos
AWS_REGION=us-east-1
RDS_CA_PEM=\"$(cat rds-ca-bundle.pem)\"

INTERNAL_API_KEY=your_64_character_random_string_here
